# The loopback network interface
 auto lo
 iface lo inet loopback
   address 10.0.0.41/32

# The primary network interface
 auto eth0
 iface eth0 inet dhcp
    vrf mgmt

auto mgmt
iface mgmt
    address 127.0.0.1/8
    vrf-table auto

auto swp44
iface swp44
   address 169.254.34.98/30

auto swp51
iface swp51

auto swp52
iface swp52

